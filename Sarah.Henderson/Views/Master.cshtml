@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ClientDependency.Core.Mvc;
@{
    Layout = null;
    Html.RequiresCss("~/css/main.css", 10);
    Html.RequiresCss("~/css/external/font-awesome.min.css", 20);
